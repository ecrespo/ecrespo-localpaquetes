	function EditRow(row){
	var f = document.getElementById("frmVS")
	
	f.editRow.value = row
	var r = rDat[row].split(",")
	EnableRow(f, r[0])
	f.name.value = r[1]
	f.ip.value = r[2]
	GetProto(f, r[3], "protocol")
	f.priPort.value = r[4]
	f.pubPort.value = r[5]
	GetSchedule(f, r, 6)
	SelectRow("tabVS",row)
	}
	
	function send_request(){
	var ip = get_by_id("ip").value;
 var temp_ip_obj = new addr_obj(ip.split("."), IP_ADDRESS_DESC, false, INVALID_IP_ADDRESS, ZERO_IP_ADDRESS, -1, false);
 
 if (get_by_id("name").value == ""){
 alert(msg[NAME_ERROR]);
 }else if (!check_address(temp_ip_obj)){
 return;
 }else if (!check_port(get_by_id("priPort").value)){
 alert(msg[PRIVATE_PORT_ERROR]);
 }else if (!check_port(get_by_id("pubPort").value)){
 alert(msg[PUBLIC_PORT_ERROR]);
 }else{
 send_submit("frmVS");
 }
	}
